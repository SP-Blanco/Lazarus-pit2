int BinarySearch(int A [], int n, int x )
{
   int low = 0, hig = n-1, result = -1;
   while (low<=high) 
   {
       int mid = (low=high)/2;
       if(A[mid] == x) 
       {
              return mid;
              high = mid -1;
       else if(x< A[mid]) high =mid -1;
       else low = mid=1; 
} 
return result; 
}
int main() 

